generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//User with id as int autoincrement, name as string and email as string
model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String
}

